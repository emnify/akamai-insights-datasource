{
  "name": "akamai-insights-datasource",
  "version": "1.2.4",
  "description": "Easy way to access your akamai data",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "server": "docker-compose up --build",
    "server:dev": "mage -v && npm run server",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "concurrently --success first npm:server \"wait-on http://localhost:3000 && open http://localhost:3000\""
  },
  "author": "Akamai",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-test-renderer": "^18.0.7",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-newlines": "^1.2.3",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "wait-on": "^7.2.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "@emotion/styled": "^11.11.0",
    "@grafana/data": "^11.3.0",
    "@grafana/runtime": "^11.3.0",
    "@grafana/schema": "^11.3.0",
    "@grafana/ui": "^11.3.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "2.5.3"
  },
  "overrides": {
    "prismjs": "1.30.0"
  },
  "packageManager": "npm@10.2.5"
}
